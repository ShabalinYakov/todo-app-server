{
  "name": "todo-app-server",
  "version": "1.0.0",
  "repository": "https://github.com/ShabalinYakov/todo-app-server.git",
  "author": "Shabalin_Yakov <shab.yakov@ya.ru>",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && cross-env NODE_ENV=production node build/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d build --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "husky install",
    "make:seeder": "knex seed:make --knexfile=knexfile.ts -x ts --env development",
    "make:migration": "knex migrate:make --knexfile=knexfile.ts -x ts --env development",
    "seed": "cross-env NODE_ENV=production knex seed:run --knexfile=knexfile.ts --verbose",
    "migrate": "cross-env NODE_ENV=production knex migrate:latest --knexfile=knexfile.ts --verbose",
    "rollback": "cross-env NODE_ENV=production knex migrate:rollback --knexfile=knexfile.ts --verbose",
    "dev:migrate": "cross-env NODE_ENV=development knex migrate:latest --knexfile=knexfile.ts --verbose",
    "dev:seed": "cross-env NODE_ENV=development knex seed:run --knexfile=knexfile.ts --verbose",
    "dev:rollback": "cross-env NODE_ENV=development knex migrate:rollback --knexfile=knexfile.ts --verbose"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.1",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.67",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "node-config": "^0.0.2",
    "nodemon": "^1.14.9",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
